{
  "plugins": ["prettier-plugin-astro"],
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "printWidth": 500,
  "singleAttributePerLine": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.astro",
      "options": { "parser": "astro", "tabWidth": 2 }
    }
  ]
}
